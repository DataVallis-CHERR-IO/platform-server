{
    "name": "cherrio-platform-server",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "rm -rf dist && yarn build && nest start",
        "start:dev": "env-cmd -f ./src/config/env/dev.env nest start --watch",
        "start:debug": "env-cmd -f ./src/config/env/dev.env nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@nestjs/apollo": "^10.1.3",
        "@nestjs/axios": "^0.1.0",
        "@nestjs/common": "^9.1.2",
        "@nestjs/core": "^9.1.2",
        "@nestjs/graphql": "^10.1.3",
        "@nestjs/passport": "^9.0.0",
        "@nestjs/platform-express": "^9.2.0",
        "@nestjs/typeorm": "^9.0.1",
        "@pollinationx/core": "^0.3.0",
        "apollo-server-express": "^3.11.1",
        "body-parser": "^1.20.1",
        "class-transformer": "^0.5.1",
        "ethers": "^5.7.2",
        "graphql": "^16.6.0",
        "graphql-subscriptions": "^2.0.0",
        "hardhat": "^2.12.2",
        "i18n": "^0.15.1",
        "json-stringify-safe": "^5.0.1",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "mysql2": "^2.3.3",
        "nest-winston": "^1.7.0",
        "nodemailer": "^6.8.0",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.0",
        "pollinationx": "^0.0.4",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.5.7",
        "tedis": "^0.1.12",
        "ts-morph": "16.0.0",
        "typeorm": "^0.3.10",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.1.4",
        "@next/eslint-plugin-next": "^13.0.4",
        "@nomiclabs/hardhat-ethers": "^2.2.1",
        "@nomiclabs/hardhat-etherscan": "^3.1.2",
        "@nomiclabs/hardhat-waffle": "^2.0.3",
        "@types/node": "^18.8.0",
        "@types/nodemailer": "^6.4.6",
        "@types/passport-jwt": "^3.0.7",
        "@typescript-eslint/eslint-plugin": "^5.38.1",
        "@typescript-eslint/parser": "^5.38.1",
        "dotenv": "^16.0.3",
        "env-cmd": "^10.1.0",
        "eslint": "8.22.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.11",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-simple-import-sort": "^8.0.0",
        "jest": "^29.1.2",
        "prettier": "^2.7.1",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
